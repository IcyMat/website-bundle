{
  "name": "icymat/website-bundle",
  "type": "symfony-bundle",
  "description": "IcyMat Simple Website Bundle",
  "keywords": ["Website management"],
  "homepage": "http://icymat.pl",
  "license": "MIT",
  "authors": [
    {
      "name": "Mateusz Kolasa",
      "email": "kontakt@mkolasa.eu"
    }
  ],
  "require": {
    "php": "^5.5.9 || ^7.0",
    "symfony/framework-bundle": "^2.7 || ^3.0",
    "symfony/twig-bundle": "^2.7 || ^3.0",
    "symfony/templating": "^2.7 || ^3.0",
    "symfony/http-foundation": "^2.7 || ^3.0",
    "symfony/routing": "^2.7 || ^3.0"
  },
  "require-dev": {},
  "conflict": {},
  "autoload": {
    "psr-4": {
      "IcyMat\\WebsiteBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "IcyMat\\WebsiteBundle\\Tests\\": "Tests/"
    }
  },
  "support": {
    "docs": "https://icymat.pl#contact"
  }
}
